{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ],
  "rules": {
    "jsx-boolean-value": false,
    "prettier": true,
    "interface-name": false,
    "no-namespace": false,
    "no-shadowed-variable": false
  }
}
